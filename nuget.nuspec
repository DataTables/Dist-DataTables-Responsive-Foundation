<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>3.0.5</version>
        <id>datatables.net-responsive-zf</id>
        <description>This is Responsive for DataTables with styling for [Foundation](https://get.foundation/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Responsive-Foundation.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>Responsive  Foundation  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-responsive" version="3.0.5"/>
            <dependency id="datatables.net-zf" version="2.3.2"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/responsive.foundation.js" target="content\Scripts"/>
        <file src="js/responsive.foundation.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/responsive.foundation.min.js" target="content\Scripts"/>
        <file src="js/responsive.foundation.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/responsive.foundation.min.mjs" target="content\Scripts"/>
        <file src="js/responsive.foundation.min.mjs" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/responsive.foundation.mjs" target="content\Scripts"/>
        <file src="js/responsive.foundation.mjs" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/responsive.foundation.css" target="content\Content"/>
        <file src="css/responsive.foundation.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/responsive.foundation.min.css" target="content\Content"/>
        <file src="css/responsive.foundation.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

